{
  "name": "example/d4-module",
  "d4Shortcode": "d4_module",
  "title": "Divi 4 Module",
  "titles": "Divi 4 Modules",
  "moduleIcon": "example/module-d4",
  "category": "module",
  "attributes": {
    "module": {
      "type": "object",
      "selector": "{{selector}}",
      "default": {
        "meta": {
          "adminLabel": {
            "desktop": {
              "value": "D4 Module"
            }
          }
        }
      },
      "styleProps": {
        "bodyFont": {
          "selector": "{{selector}} .d4_module_content"
        }
      }
    },
    "title": {
      "type": "object",
      "selector": "{{selector}} .d4_module_title",
      "default": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "headingLevel": "h2"
                }
              }
            }
          }
        }
      },
      "inlineEditor": "plainText",
      "elementType": "heading",
      "childrenSanitizer": "et_core_esc_previously",
      "attributes": {
        "class": "d4_module_title"
      }
    },
    "content": {
      "type": "object",
      "selector": "{{selector}} .d4_module_content",
      "attributes": {
        "class": "d4_module_content"
      },
      "tagName": "div",
      "inlineEditor": "richText"
    },
    "image": {
      "type": "object",
      "selector": "{{selector}} .d4_module_image_wrap img"
    }
  },
  "customCssFields": {
    "title": {
      "subName": "title",
      "selectorSuffix": " .d4_module_title"
    },
    "content": {
      "subName": "content",
      "selectorSuffix": " .d4_module_content"
    }
  }
}