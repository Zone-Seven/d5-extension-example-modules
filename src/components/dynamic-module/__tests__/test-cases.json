  {
    "title": "Display only 2 posts when the numberOfPosts attribute is set to 2.",
    "args": {
      "id": "dynamic-module-2",
      "name": "example/dynamic-module",
      "childrenIds": [],
      "attrs": {
        "title": {
          "innerContent": {
            "desktop": {
              "value": "Your Title Goes Here"
            }
          }
        },
        "postItems": {
          "innerContent": {
            "desktop": {
              "value": {
                "postsNumber": "2"
              }
            }
          }
        }
      }
    }
  },
  {
    "title": "Display title style properly when the title style is set.",
    "args": {
      "id": "dynamic-module-3",
      "name": "example/dynamic-module",
      "childrenIds": [],
      "attrs": {
        "title": {
          "innerContent": {
            "desktop": {
              "value": "Your Title Goes Here"
            }
          },
          "decoration": {
            "font": {
              "font": {
                "desktop": {
                  "value": {
                    "size": "35px",
                    "headingLevel": "h1",
                    "weight": "900",
                    "style": [
                      "uppercase"
                    ]
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  {
    "title": "Display title style properly when the post title style is set.",
    "args": {
      "id": "dynamic-module-4",
      "name": "example/dynamic-module",
      "childrenIds": [],
      "attrs": {
        "title": {
          "innerContent": {
            "desktop": {
              "value": "Your Title Goes Here"
            }
          }
        },
        "postTitle": {
          "decoration": {
            "font": {
              "font": {
                "desktop": {
                  "value": {
                    "color": "#8300e9",
                    "size": "25px",
                    "headingLevel": "h2",
                    "style": [
                      "capitalize"
                    ]
                  }
                }
              }
            }
          }
        }
      }
    }
  }
